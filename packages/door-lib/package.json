{
  "name": "door-entrance",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vanilla": {
      "types": "./dist/vanilla.d.ts",
      "import": "./dist/vanilla.js",
      "require": "./dist/vanilla.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/vanilla.ts --dts --format esm,cjs --sourcemap --clean",
    "dev": "npm run build -- --watch"
  },
  "peerDependencies": {
    "@react-three/fiber": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "three": "^0.133.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/three": "^0.160.0",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3"
  }
}
