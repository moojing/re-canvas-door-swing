{
  "name": "door-entrance-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": ["packages/*"],
  "scripts": {
    "dev": "npm run dev --workspace door-entrance-sample",
    "dev:sample": "npm run dev --workspace door-entrance-sample",
    "dev:lib": "npm run dev --workspace door-entrance",
    "build": "npm run build --workspace door-entrance && npm run build --workspace door-entrance-sample",
    "build:lib": "npm run build --workspace door-entrance",
    "build:sample": "npm run build --workspace door-entrance-sample",
    "lint": "npm run lint --workspace door-entrance-sample"
  }
}
